# Ignore directories generated by Composer
drush/contrib/
vendor/

# Ignore directories created by pre_provisioning_tasks
# and DrupalProject\composer\ScriptHandler::createRequiredFiles:
# Drupal docroot and all subdirectories
web/*
# Drupal temporary and private files
tmp/
private_files/

# Ignore files generated by PhpStorm and other editors etc.
.idea/
BAPC.code-workspace

.editorconfig
.gitattributes

# Ignore Vagrant files.
.vagrant/

# Ignore MacOS
.DS_Store

# Saved SQL dumps easily recreated each development cycle
vm/saved_sql/*
mostRecentLive.sql
mostRecentDev.sql
vm/xdebug/*

# working files in vagrant dev machine
bradford-abbas.uk.sql
.s3ignore
