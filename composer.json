{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "dropzone": {
            "type": "git",
            "url": "https://github.com/iainhouston/dropzone.git"
        },
        "imagesloaded": {
            "type": "git",
            "url": "https://github.com/iainhouston/imagesloaded.git"
        },
        "masonry": {
            "type": "git",
            "url": "https://github.com/iainhouston/masonry.git"
        },
        "jsjuicebox": {
            "type": "git",
            "url": "https://github.com/iainhouston/pellucid_gallery.git"
        },
        "monoset": {
            "type": "git",
            "url": "https://github.com/iainhouston/pellucid_monoset.git"
        },
        "minutes": {
            "type": "git",
            "url": "https://github.com/iainhouston/pellucid_minutes.git"
        },
        "meeting": {
            "type": "git",
            "url": "https://github.com/iainhouston/pellucid_meeting.git"
        },
        "document": {
            "type": "git",
            "url": "https://github.com/iainhouston/pellucid_document.git"
        },
        "agenda": {
            "type": "git",
            "url": "https://github.com/iainhouston/pellucid_agenda.git"
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/admin_content_notification": "^3.1",
        "drupal/cleantalk": "^3.0",
        "drupal/console": "^1.0.2",
        "drupal/contribute": "^5.0@beta",
        "drupal/core": "^8.7.6",
        "drupal/diff": "1.x-dev",
        "drupal/draggableviews": "^1.0",
        "drupal/embed": "^1.0",
        "drupal/entity_browser": "^2.0",
        "drupal/file_browser": "^1.1",
        "drupal/honeypot": "^1.29",
        "drupal/juicebox": "2.0-beta3",
        "drupal/mailsystem": "4.1",
        "drupal/pathauto": "^1.3",
        "drupal/simplenews": "1.x-dev",
        "drupal/swiftmailer": "1.x-dev",
        "drupal/token": "^1.5",
        "drupal/webform": "^5.0",
        "drush/drush": "~9.0",
        "iainhouston/dropzone": "dev-master",
        "iainhouston/imagesloaded": "dev-master",
        "iainhouston/masonry": "dev-master",
        "iainhouston/pellucid_agenda": "dev-master",
        "iainhouston/pellucid_document": "dev-master",
        "iainhouston/pellucid_gallery": "dev-master",
        "iainhouston/pellucid_meeting": "dev-master",
        "iainhouston/pellucid_minutes": "dev-master",
        "iainhouston/pellucid_monoset": "^1.1",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/devel": "^1.0",
        "drupal/link_css": "1.x-dev",
        "geerlingguy/drupal-vm": "4.8.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        }
    }
}
